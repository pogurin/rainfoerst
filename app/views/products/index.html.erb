<h1 id="title">ALL PRODUCTS</h1>


	<section>
	<% @products.each do |product| %>

		<div class="photo_name">

			<h4 class="create_products">
				<%= link_to product.name, product_path(product.id) %> 
			</h4>
			

			<%= link_to image_tag(product.url), product_path(product.id) %>
		</div>
	<% end %>

	</section>




